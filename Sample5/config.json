{
  "prius": {
    "databases": [
      {
        "name": "authorization",
        "type": "MySql",
        "connectionString": "Server=localhost;Database=authorization;CheckParameters=false;Uid=svc_test;Pwd=password;"
      },
      {
        "name": "identity",
        "type": "MySql",
        "connectionString": "Server=localhost;Database=identity;CheckParameters=false;Uid=svc_test;Pwd=password;"
      }
    ],
    "fallbackPolicies": [
      {
        "name": "none",
        "allowedFailurePercent": 100
      }
    ],
    "repositories": [
      {
        "name": "Authorization",
        "clusters": [
          {
            "databases": [
              "authorization"
            ],
            "fallbackPolicy": "none"
          }
        ]
      },
      {
        "name": "Identity",
        "clusters": [
          {
            "databases": [
              "identity"
            ],
            "fallbackPolicy": "noFallback"
          }
        ]
      }
    ]
  },
  "owinFramework": {
    "facility": {
      "identityStore.Prius": {
        "priusRepositoryName": "Identity",
        "identityUrnNamespace": "sample5.user",
        "mixedCaseTokens": false,
        "mixedCaseIdentity": false,
        "mixedCaseSharedSecret": true,
        "minimumUserNameLength": 3,
        "maximumUserNameLength": 80,
        "userNameRegex ": "^[a-zA-Z0-9@_.-]*$",
        "minimumPasswordLength": 8,
        "maximumPasswordLength": 160,
        "passwordRegex": ".*",
        "failedLoginsToLock": 5,
        "lockDuration": "04:00:00",
        "rememberMeFor": "90"
      }
    },
    "authorization": {
      "data": {
        "priusMasterRepository": "Authorization",
        "priusReadonlyReplicaRepository": "Authorization",
        "defaultGroup": "sys.users",
        "administratorGroup": "sys.admins",
        "anonymousGroup": ""
      }
    },
    "pages": {
      "framework": {
        "debugLogging": true,
        "debugLibraries": true,
        "defaultLanguage": "en-US",
        "assetRootPath": "/assets",
        "servicesRootPath": "/services",
        "assetCacheTime": "01:00:00",
        "assetVersion": 1,
        "templateUrlRootPath": "/",
        "templateRootPath": "/page/fromUrl"
      },
      "html": {
        "includeComments": true,
        "indented": true
      }
    },
    "middleware": {
      "traceFilter": {
        "level": "All",
        "_middlewareClasses": [
          "OwinFramework.Session.CacheSessionMiddleware",
          "OwinFramework.Session.InProcessSessionMidleware",
          "OwinFramework.FormIdentification.FormIdentificationMiddleware",
          "OwinFramework.OutputCache.OutputCacheMiddleware",
          "OwinFramework.StaticFiles.StaticFilesMiddleware",
          "OwinFramework.Versioning.VersioningMiddleware"
        ]
      }
    }
  },
  "middleware": {
    "pages": {
      "debugInfo": {}
    },
    "redirect": {},
    "notFound": {},
    "exceptions": {},
    "outputCache": {},
    "versioning": {},
    "session": {},
    "identification": {
      "forms": {
        "cookieName": "identification",
        "secureProtocol": "https",
        "secureDomain": "",
        "signupPage": "/account/register",
        "signupSuccessPage": "/account/register-success",
        "signupFailPage": "/account/register-fail",
        "signinPage": "/account/login",
        "signinSuccessPage": "/account/login-success",
        "signinFailPage": "/account/login-fail",
        "signoutPage": "/account/logout",
        "signoutSuccessPage": "/",
        "changePasswordPage": "/account/change-password",
        "changePasswordSuccessPage": "/account/change-password-success",
        "changePasswordFailPage": "/account/change-password-fail",
        "changeEmailPage": "/account/change-email",
        "changeEmailSuccessPage": "/account/change-email-success",
        "changeEmailFailPage": "/account/change-email-fail",
        "sendPasswordResetPage": "/account/reset-password",
        "sendPasswordResetSuccesPage": "/account/reset-password-success",
        "sendPasswordResetFailPage": "/account/reset-password-fail",
        "verifyEmailPage": "/account/verify-email",
        "verifyEmailSuccessPage": "/account/verify-email-success",
        "verifyEmailFailPage": "/account/verify-email-fail",
        "revertEmailPage": "/account/revert",
        "revertEmailSuccessPage": "/account/revert-success",
        "revertEmailFailPage": "/account/revert-fail",
        "renewSessionPage": "/account/renew",
        "updateIdentityPage": "/account/update",
        "welcomeEmailFrom": "do-not-reply@sample5.com",
        "welcomeEmailSubject": "Thank you for creating an account"
      }
    },
    "authorization": {
      "ui": {
        "documentationRootUrl": "/authorization/ui/config",
        "apiRootUrl": "/authorization/ui/api",
        "uiRootUrl": "/authorization/ui",
        "permissionToCallApi": "auth:ui",
        "permissionToEditPermissions": "auth:permission.edit",
        "permissionToEditRoles": "auth:role.edit",
        "permissionToEditGroups": "auth:group.edit",
        "permissionToAssignPermissionToRole": "auth:permission.assign",
        "permissionToAssignRoleToGroup": "auth:role.assign",
        "permissionToAssignUserToGroup": "auth:group.assign"
      }
    },
    "staticFiles": {
      "scripts": {
        "staticFilesRootUrl": "/scripts",
        "documentationRootUrl": null,
        "rootDirectory": "~\\web\\scripts",
        "enabled": "true",
        "includeSubFolders": "true",
        "FileExtensions": [
          {
            "extension": ".js",
            "mimeType": "application/javascript"
          }
        ],
        "maximumFileSizeToCache": 10000,
        "totalCacheSize": 1000000,
        "maximumCacheTime": "02:00:00",
        "requiredPermission": ""
      },
      "images": {
        "staticFilesRootUrl": "/images",
        "documentationRootUrl": null,
        "rootDirectory": "~\\web\\images",
        "enabled": "true",
        "includeSubFolders": "true",
        "FileExtensions": [
          {
            "extension": ".ico",
            "mimeType": "image/ico"
          },
          {
            "extension": ".jpg",
            "mimeType": "image/jpeg"
          },
          {
            "extension": ".jpeg",
            "mimeType": "image/jpeg"
          },
          {
            "extension": ".png",
            "mimeType": "image/png"
          },
          {
            "extension": ".eps",
            "mimeType": "application/postscript"
          },
          {
            "extension": ".svg",
            "mimeType": "image/svg+xml"
          }
        ],
        "maximumFileSizeToCache": 10000,
        "totalCacheSize": 1000000,
        "maximumCacheTime": "02:00:00",
        "requiredPermission": ""
      }
    },
    "less": {
      "rootUrl": "/styles",
      "rootDirectory": "~\\web\\styles",
      "enabled": true,
      "analyticsEnabled": false,
      "minify": false,
      "traceLog": false
    }
  }
}